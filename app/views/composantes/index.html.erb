<h1>Listing composantes</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Code</th>
    <th>Libelle</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @composantes.each do |composante| %>
  <tr>
    <td><%= composante.id %></td>
    <td><%= composante.code %></td>
    <td><%= composante.libelle %></td>
    <td><%= link_to 'Show', composante %></td>
    <td><%= link_to 'Edit', edit_composante_path(composante) %></td>
    <td><%= link_to 'Destroy', composante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Composante', new_composante_path %>

